    "axios": "^0.27.2",
    "i18next": "^21.8.14",
    "lorem-ipsum": "^2.0.8",
    "lottie-ios": "3.2.3",
    "react": "18.0.0",
    "react-content-loader": "^6.2.0",
    "react-i18next": "^11.18.1",
    "react-native": "0.69.1",
    "react-native-dotenv": "^3.3.1",
    "react-native-icomoon": "^0.1.1",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-lottie-splash-screen": "^1.0.1",
